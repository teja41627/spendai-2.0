{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://spendai-20-production.up.railway.app/api/$1"
        },
        {
            "source": "/v1/(.*)",
            "destination": "https://spendai-20-production.up.railway.app/v1/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}